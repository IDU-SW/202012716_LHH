<html>
<head>
   <meta charset="UTF-8" />
    <title>food_list</title>
</head>
<body>
    <h1>(음식목록)</h1>
    <p>
        <input type="button" value="추가" style="width:80" onclick="location.href='/food/add'">
    </p>
    <% foods.forEach(function(item) { %>
    <h3><a href='/foods/<%= item.id %>'>번호 : <%= item.id %></a></h3>
    
        음식 : <%= item.food %>  <br>
        분류 : <%= item.kind %>  <br>
    
    <% }) %>
</body>
</html>